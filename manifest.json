{
	"name": "TimeM",
	"version": "1.0",
	"manifest_version": 2,
	"options_page": "options.html",
	"permissions": [
          "history",
          "tabs"
        ],
	"description": "This is a short description for our chrome extension",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "mypopup.html"
	},
	"background": {"script": "history.js"}

}